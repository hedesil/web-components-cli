{
  "name": "web-components-angular-cli",
  "description": "Web Component generator (based on Angular 7 elements)",
  "version": "0.0.3",
  "author": "Mariano Moreno",
  "bin": {
    "webcomp": "./bin/run"
  },
  "bugs": "https://github.com/hedesil/web-components-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/plugin-help": "^2.1.6",
    "listr": "^0.14.3",
    "chalk": "^2.4.2",
    "execa": "^1.0.0",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@types/node": "^10.14.4",
    "@types/lodash": "^4.14.123",
    "@types/inquirer": "^6.0.0",
    "@types/listr": "^0.13.0",
    "@types/execa": "^0.9.0",
    "lodash": "^4.17.11",
    "inquirer": "^6.2.2",
    "globby": "^8.0.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hedesil/web-components-cli",
  "keywords": [
    "oclif",
    "webcomp",
    "webc component"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "webcomp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "hedesil/web-components-cli",
  "scripts": {
    "build": "npm run prepack && npm run postpack",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
